import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
     Scanner obj=new Scanner(System.in);
        int f=obj.nextInt();
        System.out.printf("Bird said:%d",f);
        System.out.printf("\nAlice must go in path-%d",(f%10)+(f/10));
        
    }
}
